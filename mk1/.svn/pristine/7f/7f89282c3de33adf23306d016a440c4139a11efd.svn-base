package com.aisd;

public class OneWayList {
    private OneWayListElement start = null;
    private int length = 0;

    public OneWayListElement getStart(){
        return start;
    }

    public void setStart(OneWayListElement newStart){
        start = newStart;
    }

    public int getLength(){
        return length;
    }

    public void incrementLength(){
        length++;
    }
}
